<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>HTS Linkage</h:title>
    <model>
      <instance>
        <hts_linkage delimiter="#" id="hts_linkage" prefix="J1!hts_linkage!" version="2020-08-11 09:40:31">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <name/>
              <contact>
                <_id/>
                <name/>
              </contact>
              <parent>
                <parent>
                  <contact>
                    <name/>
                    <phone/>
                  </contact>
                </parent>
              </parent>
            </contact>
          </inputs>
          <source/>
          <source_id/>
          <place_id/>
          <place_name/>
          <head/>
          <hts_linkage_assessment>
            <_163137_encounterDAte_99DCT/>
            <_162724_facilityLinkedTo_99DCT/>
            <_162053_cccNumber_99DCT/>
            <_1473_healthWorkerHandedTo_99DCT/>
            <_162577_cadreOfHealthWorker_99DCT/>
            <_164401_otherCadre_99DCT/>
            <_160555_dateEnrolled_99DCT/>
            <_159599_ARTdate_99DCT/>
            <_163042_remarks_99DCT/>
          </hts_linkage_assessment>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </hts_linkage>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/hts_linkage/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/hts_linkage/inputs/source" type="string"/>
      <bind nodeset="/hts_linkage/inputs/source_id" type="string"/>
      <bind nodeset="/hts_linkage/inputs/contact/_id" type="db:clinic"/>
      <bind nodeset="/hts_linkage/inputs/contact/name" type="string"/>
      <bind nodeset="/hts_linkage/inputs/contact/contact/_id" type="db:person"/>
      <bind nodeset="/hts_linkage/inputs/contact/contact/name" type="string"/>
      <bind nodeset="/hts_linkage/inputs/contact/parent/parent/contact/name" type="string"/>
      <bind nodeset="/hts_linkage/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind calculate="../inputs/source" nodeset="/hts_linkage/source" type="string"/>
      <bind calculate="../inputs/source_id" nodeset="/hts_linkage/source_id" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/hts_linkage/place_id" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/hts_linkage/place_name" type="string"/>
      <bind calculate="../inputs/contact/contact/name" nodeset="/hts_linkage/head" type="string"/>
      <bind nodeset="/hts_linkage/hts_linkage_assessment/_163137_encounterDAte_99DCT" required="true()" type="date"/>
      <bind nodeset="/hts_linkage/hts_linkage_assessment/_162724_facilityLinkedTo_99DCT" required="true()" type="string"/>
      <bind nodeset="/hts_linkage/hts_linkage_assessment/_162053_cccNumber_99DCT" required="true()" type="string"/>
      <bind nodeset="/hts_linkage/hts_linkage_assessment/_1473_healthWorkerHandedTo_99DCT" required="true()" type="string"/>
      <bind nodeset="/hts_linkage/hts_linkage_assessment/_162577_cadreOfHealthWorker_99DCT" type="select1"/>
      <bind nodeset="/hts_linkage/hts_linkage_assessment/_164401_otherCadre_99DCT" relevant=" /hts_linkage/hts_linkage_assessment/_162577_cadreOfHealthWorker_99DCT =&quot;_5622_other_99DCT&quot;" type="string"/>
      <bind nodeset="/hts_linkage/hts_linkage_assessment/_160555_dateEnrolled_99DCT" required="true()" type="date"/>
      <bind nodeset="/hts_linkage/hts_linkage_assessment/_159599_ARTdate_99DCT" required="true()" type="date"/>
      <bind nodeset="/hts_linkage/hts_linkage_assessment/_163042_remarks_99DCT" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/hts_linkage/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/hts_linkage/inputs">
      <label>Patient</label>
      <group ref="/hts_linkage/inputs/contact">
        <label>Contact</label>
        <input appearance="db-object" ref="/hts_linkage/inputs/contact/_id">
          <label>What is the patient's name?</label>
        </input>
        <group ref="/hts_linkage/inputs/contact/contact">
          <label>Contact</label>
          <input appearance="db-object" ref="/hts_linkage/inputs/contact/contact/_id">
            <label>ID of head of household</label>
          </input>
        </group>
        <group ref="/hts_linkage/inputs/contact/parent">
          <label>Parent</label>
          <group ref="/hts_linkage/inputs/contact/parent/parent">
            <label>Parent</label>
            <group ref="/hts_linkage/inputs/contact/parent/parent/contact">
              <label>Contact</label>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/hts_linkage/hts_linkage_assessment">
      <label>Linkage to care</label>
      <input ref="/hts_linkage/hts_linkage_assessment/_163137_encounterDAte_99DCT">
        <label>Encounter Date</label>
      </input>
      <input ref="/hts_linkage/hts_linkage_assessment/_162724_facilityLinkedTo_99DCT">
        <label>Facility linked to:</label>
      </input>
      <input ref="/hts_linkage/hts_linkage_assessment/_162053_cccNumber_99DCT">
        <label>CCC number:</label>
      </input>
      <input ref="/hts_linkage/hts_linkage_assessment/_1473_healthWorkerHandedTo_99DCT">
        <label>Health worker handed to</label>
      </input>
      <select1 ref="/hts_linkage/hts_linkage_assessment/_162577_cadreOfHealthWorker_99DCT">
        <label>Cadre of health worker :</label>
        <item>
          <label>Nurse</label>
          <value>_1577_nurse_99DCT</value>
        </item>
        <item>
          <label>Clincal officer/Docter</label>
          <value>_1574_clinicalOficer_99DCT</value>
        </item>
        <item>
          <label>Community health care worker</label>
          <value>_1555_communityHealthWorker_99DCT</value>
        </item>
        <item>
          <label>Employee</label>
          <value>_1540_employess_99DCT</value>
        </item>
        <item>
          <label>Other</label>
          <value>_5622_other_99DCT</value>
        </item>
      </select1>
      <input ref="/hts_linkage/hts_linkage_assessment/_164401_otherCadre_99DCT">
        <label>Specify other cadre</label>
      </input>
      <input ref="/hts_linkage/hts_linkage_assessment/_160555_dateEnrolled_99DCT">
        <label>Date enrolled :</label>
      </input>
      <input ref="/hts_linkage/hts_linkage_assessment/_159599_ARTdate_99DCT">
        <label>ART start date</label>
      </input>
      <input ref="/hts_linkage/hts_linkage_assessment/_163042_remarks_99DCT">
        <label>Remark(s)</label>
      </input>
    </group>
  </h:body>
</h:html>
